{
    "extends": "@tsconfig/node22/tsconfig.json",
    "compilerOptions": {
        "lib": [
            "es2023"
        ],
        "module": "Node16",
        "moduleResolution": "Node16",
        "target": "ES2023",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "allowJs": true,
        "resolveJsonModule": false,
        "baseUrl": ".",
        "paths": {},
        "outDir": "./dist"
    },
    "exclude": [
        "./bin",
        "./coverage",
        "tmp"
    ]
}